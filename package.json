{
  "name": "chesslog-renderer",
  "version": "0.1.0",
  "description": "A JupyterLab extension for viewing FEN format chessboard positions files.",
  "author": "sfc",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.{gif,html,jpg,json,png,svg,woff2,ttf,css}"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.9.0",
    "@jupyterlab/rendermime-interfaces": "^0.3.0",
    "@phosphor/widgets": "^1.5.0",
    "fen-chess-board ": "^2.0.4"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
